{{ attach_library('acp/frontpage-scripts') }}
{% include '@acp/header.html.twig' %}

<main
	role="main">
	{# Hero Section (already cleaned) #}
	<section class="relative -mt-16 h-[100vh] pb-24 xl:pb-0 lg:h-[90vh] w-full overflow-hidden"><!-- Background Video -->
		{# <video class="absolute inset-0 min-h-full w-full h-full object-cover" autoplay loop muted playsinline>																													<source src="{{ directory }}/assets/hero-video.MP4" type="video/mp4"/>
																																															{{ 'Your browser does not support the video tag.'|t }}
																																												</video> #}{{page.heroVideo}}<div class="absolute inset-0 bg-black/40"> </div>
		<!-- Hero Content -->
		<div class="container relative mx-auto z-10 h-full flex items-end px-8 pb-20">
  <div class="hero-text">
    <h1 id="hero-title" class="opacity-0 translate-y-8 text-4xl sm:text-[51px] font-bold text-white">
      {{ page.heroheading|render|striptags }}
    </h1>
  </div>
</div>

	</section>

	{# New Air Connectivity Section (clean, no hidden classes) #}
	<section
		class="pb-16 pt-44 sm:py-20 lg:pb-24 sm:pt-52 bg-[#EDEDED] rounded-3xl mt-[-5rem] xl:mt-[-1rem] relative z-20">
		<!-- Panel container -->
		<div id="panel-container" class="absolute -top-18 md:-top-15 left-[5%] max-w-3xl mx-auto pr-4 py-8">
			<section
				class="relative bg-[#596b74] rounded-2xl px-8 sm:px-10 py-4 md:py-8 shadow-md overflow-visible">
				<!-- green accent bar -->
				<span class="absolute -top-2 md:-top-3 left-6 md:left-8 w-28 md:w-36 h-2 md:h-3 bg-lime-400 rounded-sm"></span>

				<div>
					<div class="w-full">
						<h1 class="text-white text-lg md:text-xl lg:text-2xl font-semibold" data-i18n="content.discoverKingdom">
							{{page.heroBoxContent}}

						</h1>

						{% if language.id == 'ar' %}
							<div class="mt-6 flex flex-wrap items-start gap-4">
								<div class="flex flex-col">
									<a href="{{ url('<front>') }}/node/3" class="text-xs inline-flex items-center justify-center px-5 py-2 rounded-md bg-lime-400 text-gray-900 font-semibold shadow-sm hover:brightness-95 transition">
										كن شريكاً
									</a>
								</div>
								<div class="flex flex-col">
									<a href="#routes" class="text-xs inline-flex items-center justify-center px-5 py-2 rounded-md border border-white/60 text-white font-medium hover:bg-white/5 transition">
										اكتشف المسارات
									</a>
								</div>
							</div>
						{% else %}
							<div class="mt-6 flex flex-wrap items-start gap-4">
								<div class="flex flex-col">
									<a href="{{ url('<front>') }}become-partner" class="text-xs inline-flex items-center justify-center px-5 py-2 rounded-md bg-lime-400 text-gray-900 font-semibold shadow-sm hover:brightness-95 transition">
										Become a Partner
									</a>
								</div>
								<div class="flex flex-col">
									<a href="#routes" class="text-xs inline-flex items-center justify-center px-5 py-2 rounded-md border border-white/60 text-white font-medium hover:bg-white/5 transition">
										Explore Routes
									</a>
								</div>
							</div>
						{% endif %}

					</div>
				</div>
			</section>
		</div>

		<div class="relative container mx-auto px-4 sm:px-6 lg:px-8" id="animated-section">
			<div
				class="max-w-7xl mx-auto text-center">
				<!-- Section Heading -->
				<h2 class="text-3xl sm:text-[51px] font-bold text-[#44505E] mb-8 text-balance" data-i18n="sections.airConnectivityProgram">
					{{ page.aboutHeading|render|striptags }}
				</h2>

				<!-- Section Description -->
				<p id="gsap-text" dir="ltr" class="sm:!text-3xl !text-2xl text-gray-600 leading-tight text-pretty" data-i18n="sections.programDescription">
					{{ page.aboutDesc|render|striptags }}
				</p>
			</div>
		</div>
	</section>

	{# Stats Boxes #}
	<section class="py-16 sm:py-20 lg:py-32 bg-[#F6F6F6] overflow-hidden">
		<div class="container mx-auto px-4 sm:px-6 lg:px-8">
			<div
				class="grid grid-cols-1 lg:grid-cols-2 gap-12 lg:gap-16 items-center">

				<!-- Left Column - Content -->
				<div class="">
					<div
						id="statistics-content" class="space-y-6 text-center lg:text-left">
						<!-- Placeholder text -->
						{#<p#}
						{#	class="text-2xl text-black uppercase tracking-wide" data-i18n="stats.drivingGlobalConnections">#}
							{# DRIVING GLOBAL CONNECTIONS #}
						{#	{{ page.statsSubHeading|render|striptags }}#}
						{#</p>#}
						<!-- Main Heading -->
						<h2 class="text-3xl sm:text-[51px] font-bold text-black leading-tight" data-i18n="stats.saudiAirConnectivity">
							{{ page.statsHeading|render|striptags }}

						</h2>

						<!-- Subheading -->
						{#<h3 class="text-xl sm:text-xl text-black font-medium" data-i18n="stats.vision2030Initiative">#}
						{#	{{ page.statsdesc|render|striptags }}#}
						{#</h3>#}
					</div>
				</div>

				<!-- Right Column - Statistics Cards -->
				<div class="grid stats-card grid-cols-3 gap-3">
					{{ page.statsBox1|raw }}
					{{ page.statsBox2|raw }}
					{{ page.statsBox3|raw }}
					{#{{ page.statsBox4|raw }}#}
				</div>

			</div>
		</div>
	</section>
	{# Airports Dyanmic Data  #}
	<!-- Kingdom Airports Section -->
	<section class="xl:container mx-auto !pb-0 py-16 sm:py-20 lg:py-24 lg:!pb-24 bg-white">
		<div class="max-w-8xl px-4 sm:px-6 lg:px-8">
			<div
				class="grid lg:grid-cols-12 grid-rows-1 lg:p-10 lg:grid-rows-1 gap-2 lg:gap-12">
				<!-- Left Column - Narrower (4 columns) -->
				<div class="col-span-12 lg:col-span-5 space-y-6">
					<!-- Top Row - International Airport Data Card -->
					<!-- card container (matches image width & spacing) -->
						<div
						class="w-full max-w-4xl bg-[#34D19F] rounded-2xl px-6 md:px-10 py-6"> <!-- Header -->
						<div
							class="flex justify-start items-center gap-4 mb-2">
							<!-- Icon Square -->
							<div class="p-1 w-10 h-10 flex-shrink-0 bg-[#009393] rounded-md flex items-center justify-center">
								<img src="{{ base_path }}themes/custom/acp/assets/icons/airport.svg" class="object-contain w-7 h-7" alt=""/>
							</div>
							<!-- Title -->
							<h1 id="airportName" class="text-white text-left text-xl font-semibold">
								Airport Name
							</h1>
						</div>
						<!-- Divider -->
						<div class="border-t border-white/80 mb-2"></div>
						<!-- Table-like rows -->
						<div
							class="space-y-1 text-[17px] font-semibold">
							<!-- Years Row -->
							<div class="flex items-center border-b border-white/70 py-2">
								<div class="flex items-center gap-4 flex-1">
									<div class="w-8 h-8 rounded-md flex items-center justify-center">
										<img src="{{ base_path }}themes/custom/acp/assets/icons/years.svg" class="object-contain" alt=""/>
									</div>
									<span id="labelYears" class="text-white">Years</span>
								</div>
								{#<div id="year1" class="w-1/5 text-sm sm:text-base text-center text-white">#}
								{#	2023#}
								{#</div>#}
								<div id="year2" class="ml-2 w-1/5 text-sm sm:text-base text-center text-white">
									2023
								</div>
							</div>
							<!-- Flights Row (mapped from intl passengers) -->
							<div class="flex items-center border-b border-white/70 py-2">
								<div class="flex items-center gap-4 flex-1">
									<div class="w-8 h-8 rounded-md flex items-center justify-center">
										<img src="{{ base_path }}themes/custom/acp/assets/icons/plane.svg" class="object-contain" alt=""/>
									</div>
									<span id="intlLabel" class="text-white">International Passengers</span>
								</div>
								{#<div id="intl2" class="ml-2 w-1/5 text-sm sm:text-base text-center text-white">#}
								{#	111#}
								{#</div>#}
								<div class="flex justify-center gap-2 ml-2 w-1/5 text-sm sm:text-base text-center text-white" >
    								<div id="intl2" class="">
    									111
    								</div>
    							    <div>
    							        M
    							    </div>
								</div>
							</div>
							<!-- Passengers Row (mapped from domestic) -->
							<div class="flex items-center border-b border-white/70 py-2">
								<div class="flex items-center gap-4 flex-1">
									<div class="w-8 h-8 rounded-md flex items-center justify-center">
										<img src="{{ base_path }}themes/custom/acp/assets/icons/passangers.svg" class="object-contain" alt=""/>
									</div>
									<span id="domLabel" class="text-white">Domestic Passengers</span>
								</div>
								{#<div id="dom1" class="w-1/5 text-sm sm:text-base text-center text-white">#}
								{#	111#}
								{#</div>#}
								<div class="flex justify-center gap-2 ml-2 w-1/5 text-sm sm:text-base text-center text-white " >
        							<div id="dom2" class="">
    									111
    								</div>
    							    <div>
    							        M
    							    </div>
								</div>
							</div>
						</div>
					</div>


					<!-- Bottom Row - Image Box with Auto Slider -->
					<div class="relative h-92 rounded-xl overflow-hidden bg-gradient-to-br from-purple-400 to-pink-400">
						<div
							id="airport-image-slider" class="relative w-full h-full">
							<!-- Images will be populated by JavaScript -->
							<img id="airport-image" src="{{directory}}/assets/Mataratpictures/Arar.jpg" alt="Saudi Airport" class="w-full h-full object-cover transition-opacity duration-500">
						</div>
						<!-- Overlay Text -->
					</div>
					<!-- Description Text -->
				</div>

				<!-- Right Column - Map -->
				<div
					class="overflow-hidden min-h-[500px] sm:min-h-[900px] lg:min-h-[730px] col-span-12 lg:col-span-7">
