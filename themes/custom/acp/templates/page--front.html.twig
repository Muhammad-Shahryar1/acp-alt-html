	<section class="xl:container px-4 sm:px-6 lg:px-8 mx-auto" id="routes">
		<div
			class="max-w-8xl px-4 sm:px-6 lg:px-8 bg-[#F5F5F5] py-12 sm:py-16 lg:py-20 rounded-md">
			<!-- Row 1: Header -->
			<div class="flex justify-between items-center mb-8">
				<h2 class="text-3xl sm:text-[51px] font-bold text-[#44505E]" data-i18n="sections.routes">
